<project name="9rules">
  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>

  <bannerLeft>
    <name>9rules</name>
    <src>/images/9rules_logo.png</src>
    <href>index.html</href>
  </bannerLeft>  

  <custom>
    <reflowSkin>
      <theme>bootswatch-spruce</theme>
      <highlightJs>true</highlightJs>
      <brand>
        <name>9rules</name>
        <href>http://tamada.github.com/9rules</href>
      </brand>
      <slogan>Checking your codes, let's small object programming.</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <toc>top</toc>
      <topNav>Download|reports</topNav>
      <bottomNav>
        <column>Main|Download</column>
        <column>Documentation</column>
        <column>reports|modules</column>
      </bottomNav>
      <bottomDescription>
        This is a very good project doing interesting and valuable things.
      </bottomDescription>
      <pages>
        <index>
          <shortTitle>Welcome</shortTitle>
          <breadcrumbs>false</breadcrumbs>
          <toc>false</toc>
          <sections>
            <carousel />
            <thumbs>3</thumbs>
            <sidebar />
            <body />
            <columns>3</columns>
          </sections>
        </index>
        <developer-info>
          <toc>sidebar</toc>
        </developer-info>
      </pages>
    </reflowSkin>
  </custom>

  <body>
    
  </body>  
</project>
